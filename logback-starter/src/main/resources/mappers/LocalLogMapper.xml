<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="io.boncray.logback.localLogMapper">


    <insert id="insert" parameterType="io.boncray.bean.mode.log.LocalLog">
        INSERT INTO local_log(id,parent_track_id,current_track_id,service_name,logger_name,level,message,log_time)
        VALUES(
        <trim suffixOverrides=",">
            #{id},
            #{parentTrackId},
            #{currentTrackId},
            #{serviceName},
            #{loggerName},
            #{level},
            #{message},
            #{logTime},
        </trim>)
    </insert>


</mapper>